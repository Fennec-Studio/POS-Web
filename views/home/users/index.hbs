<body>
    {{> table}}

    <a href="/users/?p=3">prueba</a>

    <button onclick="prueba(1);">Prueba</button>


    <script>
        function prueba(idx) {
            fetch('/users/?p=' + idx)
            .then((response) => {
                return response.text()
            })
            .then((data) => {
                window.history.pushState({}, '', '/users?p=' + idx);
                document.querySelector('table').innerHTML = data;
            })
        }
    </script>
