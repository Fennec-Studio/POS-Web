<table>
    <thead>
        <tr>
            {{#each data.cols}}
                <th>{{name}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each data.rows}}
            <tr>
                {{#each this}}
                    <td>{{{this}}}</td>
                {{/each}}
            </tr>
        {{/each}}
</table>